package edu.demon.Object6;

interface BaseClass {
	void test();
}
public class AnonymousInnerClass {

	public static void main(String[] args) {
		int age = 8;
		BaseClass bc = new BaseClass() {
			
			@Override
			public void test() {
				System.out.println(age);
				
			}
		};
		bc.test();
		// 编译报错，因为匿名内部类使用了age，则age自动变成final
		System.out.println(age++);
	}
}
