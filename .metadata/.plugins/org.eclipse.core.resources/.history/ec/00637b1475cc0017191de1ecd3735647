package edu.demon.Object6;
class Father {
	void speak() {
		System.out.println("I am father");
	}
}
class Son extends Father {
	void speak() {
		System.out.println("I am son");
	}
}
public class MultiKinds {

	public static void main(String[] args) {
		Father father = new Son();
		father.speak();
	}

}
