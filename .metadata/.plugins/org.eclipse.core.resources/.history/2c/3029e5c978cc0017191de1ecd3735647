package edu.demon.Object6;

public class CompilePolymorphic {
	class Animal
	{
	    public void shout() { 
			System.out.println("发出动物的叫声");
		}
	}
	class Dog {
		public void shout() { 
			System.out.println("汪汪");
		}
	}
	class Cat {
		public void shout() { 
			System.out.println("喵喵");
		}
	}
	public <T> void animalShout(T t) {

	}
	public static void main(String[] args) {
		

	}

}
