package edu.demon.Object6;

class Person{  
    public String toString() {  
        String name = "Person";  
        return name;  
    }  
}  
  
class Man extends Person{  
    public String toString(){  
        String name = "Man";  
        return name;  
    }  
public class CompilePolymorphic {

	public static void main(String[] args) {
		

	}

}
